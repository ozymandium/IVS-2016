issn = {1095-323X},
keywords = {GMTI tracking,IMM,Intelligent systems,Kalman filters,Map,Motion estimation,PF,Particle filters,Particle tracking,Sampling methods,State estimation,State-space methods,Target tracking,Trajectory,UPF,UPF-based filter,Uncertainty,VS-IMM filter,VS-MMPF filter,ground moving target indicator,interacting multiple-model filter,motion model,multiple-model particle filter,target tracking,tracking filters,uncertainty modelling,unscented Kalman filter,unscented particle filter,va,variable structure systems,variable-structure filter},
mendeley-tags = {IMM,Map,PF,UPF},
